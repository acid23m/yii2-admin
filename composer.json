{
  "name": "acid23m/yii2-admin",
  "description": "Admin module for my dockerized Yii2 application.",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "admin",
    "dashboard",
    "user"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey P",
      "email": "webmaster.cipa@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.15.1",
    "yiister/yii2-gentelella": "~1.0",
    "bower-asset/bootbox": "~4.4",
    "yiisoft/yii2-swiftmailer": "~2.1",
    "kartik-v/yii2-widget-growl": "~1.1",
    "kartik-v/yii2-widget-fileinput": "~1.0",
    "vlucas/phpdotenv": "~2.4",
    "matomo/ini": "~2.0",
    "yii-dream-team/yii2-timezone-picker": "~0.1",
    "acid23m/yii2-image": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "dashboard\\": "src"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "git",
      "url": "git@github.com:acid23m/yii2-image.git"
    }
  ]
}
